{
  "projects": [
    {
      "name": "my-vite-app",
      "path": "/home/YOUR_USER/dev/my-vite-app",
      "command": "npx vite --host 0.0.0.0 --port 3000",
      "port": 3000,
      "domain": "dev-vite.example.com",
      "tunnel": false,
      "enabled": true,
      "maxMemory": "512M",
      "maxCpu": "50%"
    },
    {
      "name": "my-nextjs-app",
      "path": "/home/YOUR_USER/dev/my-nextjs-app",
      "command": "HOSTNAME=0.0.0.0 npx next dev --port 3001",
      "port": 3001,
      "domain": "dev-next.example.com",
      "tunnel": false,
      "enabled": true,
      "maxMemory": "1G",
      "maxCpu": "100%"
    },
    {
      "name": "my-tunnel-app",
      "path": "/home/YOUR_USER/dev/my-tunnel-app",
      "command": "npm run dev -- --port 3002",
      "port": 3002,
      "domain": "",
      "tunnel": true,
      "enabled": true,
      "maxMemory": "256M",
      "maxCpu": "25%"
    }
  ],
  "settings": {
    "dashboardPort": 9000,
    "dashboardDomain": "hub.example.com",
    "caddyConfigDir": "/etc/caddy/conf.d"
  }
}
